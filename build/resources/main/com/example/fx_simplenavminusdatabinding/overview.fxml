<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.fx_simplenavminusdatabinding.OverviewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText" text="Overview"/>
    <Button text="Test" onAction="#onHelloButtonClick"/>

    <Button fx:id="btnDetails" onAction="#changeView" layoutY="151.0">
        <text>Go to details view</text>
    </Button >

    <Button text="Go to empty view" onAction="#onEmptyViewButtonClick"/>
</VBox>
