<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.fx_simplenavminusdatabinding.EmptyviewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label  text="emptyview"/>
    <Label fx:id="welcomeText" text="emptyview" style="-fx-background-color: White"/>
    <Button text="Test" onAction="#onHelloButtonClick"/>

    <Button fx:id="btnDetails" onAction="#changeView" layoutY="151.0">
        <text>Go to details view</text>
    </Button >
</VBox>
